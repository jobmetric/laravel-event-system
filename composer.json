{
    "name": "jobmetric/laravel-event-system",
    "description": "This is a event system management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "event",
        "event-manager",
        "management"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-event-system",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.26"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\EventSystem\\": "src/",
            "JobMetric\\EventSystem\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobMetric\\EventSystem\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\EventSystem\\EventSystemServiceProvider"
            ],
            "aliases": {
                "EventSystem": "JobMetric\\EventSystem\\Facades\\EventSystem"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-event-system/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-event-system"
    }
}
